<div class="row">
  <div class="col-xs-12">
    <section id="skills">
      <div class="row">
        <div class="col-xs-12 col-sm-3">
          <div class="row">
            {{#each model.author as |dg|}}
              <div class="col-xs-12">
                {{ dribbble-author dg=dg }}
              </div>
            {{/each}}
          </div>            
        </div>  
        <div class="col-xs-12 col-sm-9">
          <div class="row">
      			{{#each model.pictures as |dg|}}
    					<div class="col-xs-12 col-sm-4">
    						{{ dribbble-pictures dg=dg }}
              </div>
    				{{/each}}
          </div>
        </div>  
      </div>
    </section>
  </div>
</div>
